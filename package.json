{
  "name": "stashtab",
  "version": "0.1.0",
  "private": true,
  "description": "Open source DeFi neobank stack - Deposit, earn, send on-chain",
  "repository": {
    "type": "git",
    "url": "https://github.com/TRC-Crypto/stashtab.git"
  },
  "license": "MIT",
  "packageManager": "pnpm@9.14.2",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "typecheck": "turbo typecheck",
    "clean": "turbo clean && rm -rf node_modules"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "prettier": "^3.1.0",
    "turbo": "^2.3.0",
    "typescript": "^5.3.0"
  }
}

